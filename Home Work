        var i = 34
        var b = (9 / 5) * i + 32
        var name = "Василий"
        var admin = name
        alert(i + " градусов по цельсию.")
        alert("Равно " + b + " градусам по фаренгейту.")
        alert("Админ сайта " + admin)
        alert(1000 + "108")
    </script>
    <h1>1000 + "108" = 1000108, потому что + поставлен не в плотную к строке 108. JavaScript не пытаеться привратить строку в число</h1>
